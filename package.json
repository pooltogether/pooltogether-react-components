{
  "name": "@pooltogether/react-components",
  "version": "1.6.31",
  "description": "PoolTogether UI elements in a shared lib",
  "author": "PoolTogether",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/pooltogether/pooltogether-react-components/issues",
    "email": "bugs@pooltogether.com"
  },
  "files": [
    "dist",
    "src",
    "config.js",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "url": "https://github.com/pooltogether/pooltogether-react-components/",
    "type": "git"
  },
  "keywords": [
    "react",
    "library",
    "components",
    "tailwindcss"
  ],
  "scripts": {
    "reinstall": "rm -rf ./node_modules && yarn",
    "start": "tsdx watch --onSuccess \"yalc push\"",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/react-components.esm.js",
  "size-limit": [
    {
      "path": "dist/react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-url": "^6.1.0",
    "@size-limit/preset-small-lib": "^6.0.4",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.9",
    "@types/node": "^16.7.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "i18next": "^21.6.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "postcss": "^8.4.6",
    "postcss-custom-properties-fallback": "^1.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^14.0.2",
    "react-i18next": "^11.15.0",
    "react-spring-bottom-sheet": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^6.0.4",
    "tailwindcss": "^3.0.23",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@download/blockies": "^1.0.3",
    "@pooltogether/evm-chains-extended": "^0.6.4-beta.1",
    "@pooltogether/hooks": "^1.4.9",
    "@pooltogether/utilities": "^0.3.8",
    "@reach/dialog": "^0.15.0",
    "@reach/menu-button": "^0.15.1",
    "@reach/popover": "^0.15.0",
    "@reach/visually-hidden": "0.15.0",
    "beautiful-react-hooks": "^1.0.2",
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.4.6",
    "feather-icons-react": "^0.4.1",
    "framer-motion": "^4.1.17",
    "jotai": "^1.4.3",
    "js-cookie": "^2.2.1",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-countup": "^6.1.0",
    "react-device-detect": "^1.15.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.22.1",
    "react-hotkeys": "^2.0.0",
    "react-toastify": "^8.2.0",
    "react-tooltip": "^4.2.21"
  }
}
