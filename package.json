{
  "name": "@pooltogether/react-components",
  "version": "1.5.0",
  "description": "PoolTogether UI elements in a shared lib",
  "author": "PoolTogether",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "config.js",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooltogether/pooltogether-react-components.git"
  },
  "keywords": [
    "react",
    "library",
    "components",
    "tailwindcss"
  ],
  "scripts": {
    "reinstall": "rm -rf ./node_modules && yarn",
    "start": "tsdx watch --onSuccess \"yalc push\"",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/react-components.esm.js",
  "size-limit": [
    {
      "path": "dist/react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@pooltogether/current-pool-data": "^3.7.2",
    "@pooltogether/evm-chains-extended": "^0.6.4-beta.1",
    "@pooltogether/hooks": "^1.4.2",
    "@pooltogether/utilities": "^0.3.4",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.4.6",
    "feather-icons-react": "^0.4.1",
    "framer-motion": "^4.1.17",
    "jotai": "^1.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "tailwindcss": "^2.2.7"
  },
  "devDependencies": {
    "@pooltogether/current-pool-data": "^3.7.2",
    "@pooltogether/evm-chains-extended": "^0.6.4-beta.1",
    "@pooltogether/hooks": "^1.4.2",
    "@pooltogether/utilities": "^0.3.4",
    "@rollup/plugin-url": "^6.1.0",
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.4.6",
    "feather-icons-react": "^0.4.1",
    "framer-motion": "^4.1.17",
    "husky": "^7.0.4",
    "jotai": "^1.4.3",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "rollup-plugin-postcss": "^4.0.1",
    "size-limit": "^6.0.4",
    "tailwindcss": "^2.2.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@download/blockies": "^1.0.3",
    "@reach/dialog": "^0.15.0",
    "@reach/menu-button": "^0.15.1",
    "@reach/popover": "^0.15.0",
    "@reach/visually-hidden": "0.15.0",
    "classnames": "^2.2.6",
    "deep-equal": "^2.0.5",
    "js-cookie": "^2.2.1",
    "react-content-loader": "^6.0.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-device-detect": "^1.15.0",
    "react-hotkeys": "^2.0.0",
    "react-loader-spinner": "^4.0.0",
